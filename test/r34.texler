# campo del archivo de entrada.
function r34()
    File "test_file_cases.txt" as input.
    File "new_r34.txt" as output.

    with input: for column in columns().lines().byIndex(1) do
                        if column is Number then
                            column * 2 -> output.
                        else
                            column -> output.
                        .
                .
    .

    return.
end

