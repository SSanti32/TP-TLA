# campo del archivo de entrada.
function r34()
    File "path" as input.
    File "new" as output.

    with input: for col in lines().byIndex(1) do
                    if col is Number then
                        col * 2 -> output.
                    else
                        col -> output.
                    .
                .
    .

    return.
end

