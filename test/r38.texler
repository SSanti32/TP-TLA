# R3.8 [Aceptar] Un programa que agrupe 5 lÃ­neas de entrada en 1 de salida
# separada por comas.
function r38()
    File "test_file_cases.txt" as input.
    File "new_r38.txt" as output.

    let accumulator be "".

    for idx in [1..5] do
        if idx == 5 then
            with input: lines().byIndex(idx) -> accumulator.
            .
        else
            with input:
                lines().byIndex(idx) -- "\n" -> accumulator.
                accumulator ++ "," -> accumulator.
            .
        .
    .

    accumulator -> output.

    return.
end

